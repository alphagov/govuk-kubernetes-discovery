apiVersion: v1
kind: Service
metadata:
  labels:
    app: <%= @application %>
  name: <%= @application %>
  namespace: <%= @config['namespace'] %>
spec:
  selector:
    app: <%= @application %>
  type: NodePort
  ports:
  - name: <%= @application %>
    protocol: TCP
    port: <%= @config['port'] %>
    targetPort: <%= @config['targetport'] %>
